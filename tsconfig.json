{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": ["webpack-env"],
    "paths": {
      "@/*": ["src/*"],
      "@common/*": ["common/*"],
      "@core/*": ["core/*"],
      "@antd-ui/*": ["antd-ui/*"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "common/**/*.ts",
    "common/**/*.tsx",
    "common/**/*.vue",
    "core/**/*.ts",
    "core/**/*.tsx",
    "core/**/*.vue",
    "antd-ui/**/*.ts",
    "antd-ui/**/*.tsx",
    "antd-ui/**/*.js",
    "antd-ui/**/*.jsx"
  ],
  "exclude": ["node_modules"]
}
